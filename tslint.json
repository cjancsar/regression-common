{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-jest", "tslint-config-prettier"],
  "jsRules": {
    "quotemark": [false],
    "member-access": [false],
    "ordered-imports": [false],
    "member-ordering": [false],
    "interface-name": [false],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "expect-expect": true,
    "variable-name": {
      "options": [
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    }
  },
  "rules": {
    "quotemark": [false],
    "member-access": [false],
    "ordered-imports": [false],
    "member-ordering": [false],
    "interface-name": [false],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "expect-expect": true
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
        "./**/coverage/**"
    ]
}
}
